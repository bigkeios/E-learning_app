<% provide(:title, 'Users List')%>
<h1> All users </h1>
<div class="container">
  <div class="row">
    <div class= "col-md-10 offset-md-1">
      <%= will_paginate @users, previous_label: 'Previous', next_label: 'Next', renderer: WillPaginate::ActionView::BootstrapLinkRenderer %>
      <ul class="users">
          <%= render @users %>
      </ul>
      <%= will_paginate @users, previous_label: 'Previous', next_label: 'Next', renderer: WillPaginate::ActionView::BootstrapLinkRenderer %>
    </div>
  </div>
</div>